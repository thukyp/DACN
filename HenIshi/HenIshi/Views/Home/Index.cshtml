    @* Chat lỏ nha khoa:)) *@
    <button class="toggle-chat" onclick="toggleChat()">Chat</button>
    <div class="chatbox" id="chatbox">
        <div class="chat-header" onclick="toggleChat()">Chatbot Nha Khoa</div>
        <div class="chat-body" id="chat-body"></div>
        <input type="text" id="chat-input" class="chat-input" placeholder="Nhập câu hỏi..." onkeypress="handleKeyPress(event)">
    </div>

    <script>
        function toggleChat() {
            var chatbox = document.getElementById("chatbox");
            if (chatbox.style.display === "none" || chatbox.style.display === "") {
                chatbox.style.display = "block";
            } else {
                chatbox.style.display = "none";
            }
        }
         function handleKeyPress(event) {
            if (event.key === "Enter") {
                var input = document.getElementById("chat-input").value;
                var chatBody = document.getElementById("chat-body");
                var response = getChatResponse(input);
                chatBody.innerHTML += `<p><strong>Bạn:</strong> ${input}</p>`;
                chatBody.innerHTML += `<p><strong>Bot:</strong> ${response}</p>`;
                document.getElementById("chat-input").value = "";
                chatBody.scrollTop = chatBody.scrollHeight;
            }
        }

        function getChatResponse(input) {
            var responses = {
                "bọc răng sứ": "Bọc răng sứ giúp cải thiện thẩm mỹ và bảo vệ răng.",
                "cấy ghép implant": "Cấy ghép implant là phương pháp thay thế răng đã mất hiệu quả.",
                "niềng răng": "Niềng răng giúp điều chỉnh vị trí răng, mang lại hàm răng đẹp.",
                "tẩy trắng răng": "Tẩy trắng răng giúp răng sáng hơn, tự tin hơn.",
                "nhổ răng khôn": "Nhổ răng khôn giúp ngăn ngừa các biến chứng về sau.",
              "tôi mắc ỉa": "Cút về nhà mà ỉa",
              "Tôi đẹp trai không": "Đep như nam thần campuchia",
              "input":"co cai con c"
            };
            return responses[input.toLowerCase()] || "Xin lỗi, tôi chưa hiểu câu hỏi của bạn. Vui lòng thử lại!";
        }
    </script>

       
