    <script>
        function delayRedirect() {
            let preloader = document.querySelector(".preloader");

            // Hiển thị preloader
            preloader.style.display = "flex";
            preloader.style.opacity = "1";

            // Vô hiệu hóa nút bấm để tránh bấm nhiều lần
            document.querySelector(".button-Login").style.pointerEvents = "none";
            setTimeout(() => {
                window.location.href = "/home/login";
            }, 2000);
        

        document.querySelector(".button-Signup").style.pointerEvents = "none";
            setTimeout(() => {
                window.location.href = "/home/login";
            }, 2000);
        }
        // Ẩn preloader khi trang tải xong
        window.onload = function () {
            let preloader = document.querySelector(".preloader");
            preloader.style.opacity = "0";
            setTimeout(() => {
                preloader.style.display = "none";
            }, 500); // Đợi 0.5s để hiệu ứng mượt hơn
        };
    </script>

